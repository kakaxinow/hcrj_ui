ted":{"line":80,"column":0}},{"source":"src/toast.vue","name":null,"original":{"line":12,"column":0},"generated":{"line":81,"column":0}}],"sources":{"src/toast.vue":"<template>\n    <div class=\"toast\" ref=\"wrap\" :class=\"toastClasses\">\n        <slot></slot>\n        <div class=\"line\" ref=\"line\"></div>\n        <span v-if=\"closeButton\" class=\"closetext\" @click=\"onClickClose\">\n            {{closeButton.text}}\n        </span>\n    </div>\n</template>\n\n<script>\n    export default {\n        name: \"toast\",\n        props:{\n            autoClose:{\n                type:Boolean,\n                default:true\n            },\n            autoCloseDelay:{\n                type:Number,\n                default: 3\n            },\n            closeButton:{\n                type:Object,\n                default(){\n                    return {\n                        text:\"关闭\",\n                        callback:undefined\n                    }\n                }\n            },\n            position:{\n                type:String,\n                default:'top',\n                validator(value){\n                    return ['top','bottom','middle'].indexOf(value)>=0\n                }\n            }\n        },\n        computed:{\n            toastClasses(){\n                return {\n                    [`position-${this.position}`]:true\n                }\n            }\n        },\n        mounted() {\n            console.log(this.autoClose);\n            if(this.autoClose){\n                setTimeout(()=>{\n                    this.close();\n                },this.autoCloseDelay*1000);\n            }\n            this.$nextTick(()=>{\n                this.$refs.line.style.height = `${this.$refs.wrap.getBoundingClientRect().height}px`;\n            })\n        },\n        methods:{\n            close(){\n                this.$el.remove();\n                this.$destroy();\n            },\n            onClickClose(){\n                this.close();\n                if(this.closeButton && typeof this.closeButton.callback==='function'){\n                    this.closeButton.callback();\n                }\n            }\n        }\n    }\n</script>\n\n<style scoped lang=\"scss\">\n    $font-size:14px;\n    $height:40px;\n    $toast-bg:rgba(0,0,0,0.75);\n\n.toast{\n    font-size: $font-size;\n    line-height:1.8;\n    min-height: $height;\n    background-color: $toast-bg;\n    border-radius: 4px;\n    color: white;\n    padding: 0 16px;\n    box-shadow: 0px 0px 3px 0px rgba(0,0,0,0.5);\n    position: fixed;\n    left: 50%;\n    display: flex;\n    align-items: center;\n    .closetext{\n        margin-left: 16px;\n        flex-shrink: 0;\n    }\n    .line{\n        height: 100%;\n        border: 1px solid #666;\n        margin-left: 6px;\n\n    }\n    &.positions-top{\n        top: 0;\n        transform: translateX(-50%);\n\n    }\n    &.positions-bottom{\n        bottom:0;\n        transform: translateX(-50%);\n\n    }\n    &.positions-middle{\n        top: 50%;\n        transform: translate(-50%,-50%);\n\n    }\n\n}\n\n</style>"},"lineCount":null},"css":".toast[data-v-69f25d] {\n  font-size: 14px;\n  line-height: 1.8;\n  min-height: 40px;\n  background-color: rgba(0, 0, 0, 0.75);\n  border-radius: 4px;\n  color: white;\n  padding: 0 16px;\n  box-shadow: 0px 0px 3px 0px rgba(0, 0, 0, 0.5);\n  position: fixed;\n  left: 50%;\n  display: flex;\n  align-items: center;\n}\n.toast .closetext[data-v-69f25d] {\n  margin-left: 16px;\n  flex-shrink: 0;\n}\n.toast .line[data-v-69f25d] {\n  height: 100%;\n  border: 1px solid #666;\n  margin-left: 6px;\n}\n.toast.positions-top[data-v-69f25d] {\n  top: 0;\n  transform: translateX(-50%);\n}\n.toast.positions-bottom[data-v-69f25d] {\n  bottom: 0;\n  transform: translateX(-50%);\n}\n.toast.positions-middle[data-v-69f25d] {\n  top: 50%;\n  transform: translate(-50%, -50%);\n}"},"hash":"525689478e7510732c56abc1bc404d19","cacheData":{"env":{}}}